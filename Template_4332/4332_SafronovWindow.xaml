<Window x:Class="Template_4332._4332_SafronovWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Template_4332"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="_4332_SafronovWindow" Height="300" Width="300">
    <StackPanel>
        <Label Content="Cафронов Михаил 18 лет" HorizontalAlignment="Center" FontSize="20"/>
        <Separator Opacity="0" Height="5"/>
        <Button x:Name="btnExport" Content="Экспорт" Click="btnExport_Click"/>
        <Separator Opacity="0" Height="5"/>
        <Button x:Name="btnImport" Content="Импорт" Click="btnImport_Click"/>
    </StackPanel>
</Window>
